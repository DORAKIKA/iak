---
import Card from "./Card.astro";

import Tabs from "./Tabs.astro";
import LinkCard from "./LinkCard.astro";
import Outline from "./Outline.astro";
import Grid from "./Grid.astro";
import GridItem from "./GridItem.astro"
import Friend from "./Friend.astro";
import Website from "./Website.astro";

import Pre from "./Pre.astro";
import Img from "./Img.astro";
import A from "./A.astro";
// 注释一下，防忘记，组件的margin标准为上下0.5rem

const { entry, source } = Astro.props;

const components: Record<string, any> = {
    Grid,
    GridItem,
    LinkCard,
    Outline,
    Tabs,
    Friend,
    Website,
    
    img: Img,
    pre: Pre,
    a: A,
}

if(source !== 'card'){
    components.Card = Card
}

const { Content } = await entry.render();
---

<Content components={components}></Content>