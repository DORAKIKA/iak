{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@styles/*": [
        "src/styles/*"
      ],
      "@mdx/*": [
        "src/components/MDX/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
      "@content/*": [
        "src/content/*"
      ],
    },
    "jsx": "preserve"
  }
}